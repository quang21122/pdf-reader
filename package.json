    "next": "15.3.5",
    "pdfjs-dist": "^5.3.31",
    "postcss": "^8.5.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-pdf": "^10.0.1",
    "tailwindcss": "^3.4.17",
